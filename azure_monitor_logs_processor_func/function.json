{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "events",
      "direction": "in",
      "eventHubName": "%EventHubName%",
      "connection": "EventHubConnection",
      "cardinality": "many",
      "consumerGroup": "%ConsumerGroupName%",
      "dataType": "binary"
    },
    {
      "name": "failedEventsOutputBlob",
      "type": "blob",
      "path": "splunk-failed-%SourceType%-logs/{DateTime:yyyy}/{DateTime:MM}/{DateTime:dd}/{DateTime:hh}/splunk-failed-%SourceType%-%Region%-logs-{DateTime:yyyy}-{DateTime:MM}-{DateTime:dd}-{DateTime:hh}].log",
      "connection": "FailedEventsStorageConnection",
      "direction": "out"
    }
  ]
}