    "chai": "4.3.4",
    "mocha": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "rewire": "5.0.0",
    "sinon": "11.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "moment": "^2.29.1"
